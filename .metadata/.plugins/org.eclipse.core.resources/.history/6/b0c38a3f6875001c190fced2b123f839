package propertiesFile;

public class DataManager {

	
	private DataManager() {
		
	}
	
	public static PropertiesDataDrive propDataDriveInstance() {
		PropertiesDataDrive pdd = new PropertiesDataDrive();
		return pdd;
		
	}
	
	public DataManager dataManagerInstance() {
		DataManager dm = new DataManager();
		return dm;
	}
}
